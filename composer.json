{
    "name": "your-project-name",
    "description": "Your project description",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "phpactor/phpactor": "^0.15.3"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourNamespace\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

